<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.gui.controllergui.creation.CreateOrJoinGUI">
    <ImageView pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Image url="@../png/background_1.jpg" />
    </ImageView>
    <HBox alignment="CENTER" layoutX="100.0" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <Button fx:id="createBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="29.0" prefWidth="119.0" text="Create Game" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets left="30.0" right="30.0" />
            </HBox.margin></Button>
        <Button fx:id="JoinBtn" maxWidth="1.7976931348623157E308" prefHeight="29.0" prefWidth="119.0" text="Join Game" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets left="30.0" right="30.0" />
            </HBox.margin></Button>
    </HBox>
</AnchorPane>
